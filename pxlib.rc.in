#include <windows.h>

#define PXLIB_MAJOR_VERSION @PXLIB_MAJOR_VERSION@
#define PXLIB_MINOR_VERSION @PXLIB_MINOR_VERSION@
#define PXLIB_MICRO_VERSION @PXLIB_MICRO_VERSION@
#define PXLIB_DOTTED_VERSION "@PXLIB_DOTTED_VERSION@"

VS_VERSION_INFO VERSIONINFO
 FILEVERSION PXLIB_MAJOR_VERSION,PXLIB_MINOR_VERSION,PXLIB_MICRO_VERSION,0
 PRODUCTVERSION PXLIB_MAJOR_VERSION,PXLIB_MINOR_VERSION,PXLIB_MICRO_VERSION,0
 FILEFLAGSMASK VS_FFI_FILEFLAGSMASK
#ifdef _DEBUG
 FILEFLAGS VS_FF_DEBUG
#else
 FILEFLAGS 0x0L
#endif
 FILEOS VOS_NT_WINDOWS32
 FILETYPE VFT_DLL
 FILESUBTYPE VFT2_UNKNOWN
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName", "pxlib"
            VALUE "FileDescription", "Paradox database library"
            VALUE "FileVersion", PXLIB_DOTTED_VERSION
            VALUE "InternalName", "pxlib"
            VALUE "LegalCopyright", "Copyright (C) 2003-2024 Uwe Steinmann"
            VALUE "OriginalFilename", "pxlib.dll"
            VALUE "ProductName", "pxlib - Paradox database library"
            VALUE "ProductVersion", PXLIB_DOTTED_VERSION
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
